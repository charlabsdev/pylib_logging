{
    "semantic_release": {
        "assets": [],
        "branches": {
            "main": {
                "match": "main",
                "prerelease_token": "rc",
                "prerelease": false
            }
        },
        "build_command": "uv build",
        "build_command_env": [],
        "changelog": {
            "changelog_file": "",
            "default_templates": {
                "changelog_file": "CHANGELOG.md",
                "output_format": "md",
                "mask_initial_release": true
            },
            "environment": {
                "block_start_string": "{%",
                "block_end_string": "%}",
                "variable_start_string": "{{",
                "variable_end_string": "}}",
                "comment_start_string": "{#",
                "comment_end_string": "#}",
                "trim_blocks": false,
                "lstrip_blocks": false,
                "newline_sequence": "\n",
                "keep_trailing_newline": false,
                "extensions": [],
                "autoescape": false
            },
            "exclude_commit_patterns": [],
            "mode": "update",
            "insertion_flag": "<!-- version list -->",
            "template_dir": "templates"
        },
        "commit_author": {
            "env": "GIT_COMMIT_AUTHOR",
            "default": "semantic-release <semantic-release>"
        },
        "commit_message": "{version}\n\nAutomatically generated by python-semantic-release",
        "commit_parser": "conventional",
        "commit_parser_options": {
            "minor_tags": [
                "feat"
            ],
            "patch_tags": [
                "fix",
                "perf"
            ],
            "other_allowed_tags": [
                "build",
                "chore",
                "ci",
                "docs",
                "style",
                "refactor",
                "test"
            ],
            "allowed_tags": [
                "feat",
                "fix",
                "perf",
                "build",
                "chore",
                "ci",
                "docs",
                "style",
                "refactor",
                "test"
            ],
            "default_bump_level": 0,
            "parse_squash_commits": true,
            "ignore_merge_commits": true
        },
        "logging_use_named_masks": false,
        "major_on_zero": false,
        "allow_zero_version": false,
        "repo_dir": "/Users/charlesfrancois.stcyr/Development/charlabs_pylib_logging",
        "remote": {
            "name": "origin",
            "type": "github",
            "ignore_token_for_push": false,
            "insecure": false,
            "token": {
                "env": "GITHUB_TOKEN"
            }
        },
        "no_git_verify": false,
        "tag_format": "v{version}",
        "publish": {
            "dist_glob_patterns": [
                "dist/*"
            ],
            "upload_to_vcs_release": true
        },
        "version_provider": "tag",
        "version_variables": ["src/charlabs/logging/__init__.py:__version__"]
    }
}
